{
	"name": "yb-selected-posts",
	"version": "1.0.0",
	"description": "Block for adding a link to published posts.",
	"author": "Yorick Brown",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start",
		"dist": "command -v composer >/dev/null 2>&1 || { echo >&2 'Error: Composer is not installed. Please install Composer to continue.'; exit 1; } && npm run build && composer install --no-dev --optimize-autoloader && npm run plugin-zip",
		"dist:clean": "rimraf node_modules && npm run dist"
	},
	"devDependencies": {
		"@eslint/js": "^9.38.0",
		"@wordpress/scripts": "^27.8.0",
		"eslint": "^8.57.1",
		"eslint-plugin-react": "^7.37.5",
		"globals": "^16.4.0"
	},
	"dependencies": {
		"@wordpress/components": "^27.5.0",
		"@wordpress/core-data": "^6.34.0",
		"@wordpress/element": "^5.34.0",
		"@wordpress/i18n": "^4.57.0"
	},
	"files": [
		"build",
		"includes",
		"vendor",
		"yb-selected-posts.php",
		"readme.txt",
		"composer.json"
	]
}
